{% extends "admin_base.twig" %}

{% block template %}
  <h1>{{pageTitle}}</h1>
  <h2>Hasher: {{hasherName}}</h2>
      <div class="row marketing">
        <div class="col-sm-12">
          <div class="list-group">
            <a href="#" class="list-group-item active">
              Select Kennel
            </a>
            {% for kennelValue in kennelValues %}
              <a class="list-group-item" href="/{{kennelValue.KENNEL_ABBREVIATION}}/hashers/{{hasherId}}">{{kennelValue.KENNEL_ABBREVIATION}} Statistics</a>
            {% endfor %}

          </div>
        </div>
      </div>

{% endblock %}
