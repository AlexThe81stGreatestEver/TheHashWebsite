<?xml version="1.0" encoding="UTF-8" ?>

<rss version="2.0">

<channel>

  <title>Recent Hashes</title>
  <link>{{ url }}</link>
  <description>On on</description>

  {% if hashes|length > 0 %}
  <item>
    <title>Recent Hashes:
    {% set out = "" %}
    {% for item in hashes %}
      {% set out = out ~ item.TEXT ~ "; " %}
    {% endfor %}
    {{ out|trim('; ') }}
    </title>
    <link>{{url_prefix}}/listHashes2</link>
    <description>List of Events</description>
  </item>
  {% endif %}

</channel>

</rss>
