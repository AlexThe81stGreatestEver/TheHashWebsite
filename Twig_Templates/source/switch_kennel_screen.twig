{% extends "main_page.twig" %}

{% block main_page_content %}

      <div class="row marketing">
        <div class="col-sm-12">
          <div class="list-group">
            <a href="#" class="list-group-item active">
              Other Kennels
            </a>
            {% for kennelValue in kennelValues %}
              {% if kennel_abbreviation == kennelValue.KENNEL_ABBREVIATION %}
                <a class="list-group-item" href="/{{kennelValue.KENNEL_ABBREVIATION}}"><b>{{kennelValue.KENNEL_ABBREVIATION}} Statistics</b></a>
              {% else %}
                <a class="list-group-item" href="/{{kennelValue.KENNEL_ABBREVIATION}}">{{kennelValue.KENNEL_ABBREVIATION}} Statistics</a>
              {% endif %}
            {% endfor %}

          </div>
        </div>
      </div>

{% endblock %}
